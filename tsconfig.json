{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
, "src/components/Hero.tsx", "src/components/Feature.js", "src/components/Layout/Layout.jsx", "src/components/Layout/Header.tsx", "src/components/Layout/Footer.jsx", "src/components/misc/ButtonOutline.tsx", "src/components/misc/ButtonPrimary.js", "src/utils/getScrollAnimation.js"  ],
  "exclude": [
    "node_modules"
  ]
}
